proc sql;

create table xys as
select Vendor_settled_date, count(*) from Data_20220908042108958_J1
group by Vendor_settled_date;
quit;
run;


data ROLAP_J1;
SET Data_OLAP_July_J1 Data_OLAP_Aug_J1 Data_OLAP_Sep_J1;
Brand="JBO";
run;

